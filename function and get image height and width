
function square(x){
	var ans = x*x;
	return ans;
}
var y = square(4);
print("The square of x="+y);


var img=new SimpleImage("chapel.png");
print(img);
var a=img.getPixel(2,2);
print("pixel of image="+a);
var b=img.getRed(2,2);
print("Red Portion="+b);
var c=img.getGreen(2,2);
print("Green Portion="+c);
var d=img.getBlue(2,2);
print("Blue Portion="+d);
var e=img.getWidth();
print("width of image="+e);
var f=img.getHeight();
print("Height of image="+f);

