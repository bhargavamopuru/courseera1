var img = new SimpleImage("duke_blue_devil.png") print(img);

for (var pixel of img.values())
  { 
     pixel = changeToYellow(pixel);
  }

function changeToYellow(pixel)
{
   if (pixel.getRed() == 0 )
     { 
         pixel.setRed (255);
         pixel.setGreen (255);
         pixel.setBlue (0); 
     }

 }
 print(img);
